{
    "name": "soluzione-software/nova-users-tree-tool",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/config": "^5.8",
        "illuminate/database": "^5.8",
        "illuminate/support": "^5.8",
        "illuminate/view": "^5.8",
        "laravel/nova": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "SoluzioneSoftware\\Nova\\Tools\\UsersTree\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoluzioneSoftware\\Nova\\Tools\\UsersTree\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
